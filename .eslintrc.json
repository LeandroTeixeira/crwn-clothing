{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "overrides": [
      {
        "files":["**/*.test.js", "App.js","index.js"],
        "rules": {
            "react/jsx-filename-extension":"off"
        }
      }
    ]
    
}
